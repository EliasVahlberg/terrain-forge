{
  "algorithm": {
    "type": "prefab",
    "max_prefabs": 6,
    "min_spacing": 3,
    "allow_rotation": true,
    "prefabs": [
      {
        "pattern": [
          "###",
          "#.#",
          "###"
        ]
      },
      {
        "pattern": [
          "#####",
          "#...#",
          "#.#.#",
          "#...#",
          "#####"
        ]
      }
    ]
  },
  "effects": [
    {
      "name": "connect_regions_spanning",
      "config": {
        "extra_connection_chance": 0.1
      }
    }
  ],
  "validation": {
    "connectivity": true
  }
}
